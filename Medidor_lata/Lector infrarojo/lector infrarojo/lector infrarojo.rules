
(rules PCB lector infrarojo
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 709)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 4.0)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.3)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "Net-(IF1-Pad1)" "Net-(IF1-Pad3)" "Net-(IF1-Pad2)" "Net-(IF2-Pad1)" "Net-(IF3-Pad1)" "Net-(J1-Pad2)" "Net-(IF2-Pad2)" "Net-(IF3-Pad2)"
    "Net-(IF4-Pad1)" "Net-(IF4-Pad2)" "Net-(IF5-Pad2)" "Net-(IF5-Pad1)" "Net-(IF6-Pad1)" "Net-(IF6-Pad2)" "Net-(IF7-Pad2)" "Net-(IF7-Pad1)"
    "Net-(Q3-Pad3)" "Net-(R11-Pad2)" "Net-(Q2-Pad3)" "Net-(Q1-Pad3)" "Net-(Q7-Pad3)" "Net-(Q5-Pad3)" "Net-(Q6-Pad3)" "Net-(Q4-Pad3)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)